struct person { name, favcolor }

func greet(person) {
	proclaim("Hello, " + person.name + "!\n");

	if person.favcolor == "green" {
		proclaim("I like green too!\n");
	}
}

samp = person("samp", "green");
greet(samp);


func fizzBuzz (max) {
	i = 1;

	while i < max {
		if !(i % 3) { proclaim("Fizz") }
		if !(i % 5) { proclaim("Buzz") }
		if  (i%3)*(i%5) { proclaim(i) }
		i = i + 1;
		proclaim("\n");
	}
}

fizzBuzz(100)
